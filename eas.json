{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./firebase-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
